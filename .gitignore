frontend/*.pbxproj # Xcode project files

# Backend (Node.js)
backend/node_modules
backend/package-lock.json
backend/dist/
backend/.env # Environment variables (keep secret!)
backend/coverage/
backend/*.log

# Docker
.dockerignore
docker-compose.override.yml # If you have one
.env # Environment variables (keep secret!)

# OS and Editors
.DS_Store
.idea/ # IntelliJ IDEA
.vscode/ # VS Code
*.swp # Swap files (Vim)
*~ # Backup files
Thumbs.db

# Logs and temporary files
*.log
*.tmp
temp/
tmp/

# Testing
*.test.js.snap # Jest snapshots
*.test.tsx.snap # Jest snapshots

# Other
.DS_Store # macOS specific
.env # Environment variables (keep secret!) - can be moved to a sample file for the team.
*.env # Environment variables (keep secret!)
.history # Bash history
*.gz # gzip files
*.tar # tar files
*.zip # zip files
*.rar # rar files
*.7z # 7-zip files

